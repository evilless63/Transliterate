{
    "name": "evilless63/transliterate",
    "minimum-stability": "dev",
    "description": "Transliteration package for laravel",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/evilless63/Transliterate"
        }
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Eugene Dzhumak",
        "email": "elforastero@ya.ru"
      }
    ],
    "autoload": {
      "psr-4": {
        "ElForastero\\Transliterate\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "ElForastero\\Transliterate\\Tests\\": "tests/"
      }
    },
    "require": {
      "php": ">=7.3",
      "ext-intl": "*",
      "illuminate/support": "~8.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^8.0",
      "orchestra/testbench": "^4.4",
      "friendsofphp/php-cs-fixer": "^2.16",
      "brainmaestro/composer-git-hooks": "^2.8"
    },
    "extra": {
      "laravel": {
        "providers": [
          "ElForastero\\Transliterate\\ServiceProvider"
        ],
        "aliases": {
          "Transliterate": "ElForastero\\Transliterate\\Facade"
        }
      },
      "hooks": {
        "pre-commit": [
          "php-cs-fixer fix ."
        ]
      }
    },
    "scripts": {
      "test": "phpunit",
      "check-style": "php-cs-fixer fix src/ --dry-run --diff"
    }
  }
